{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": [
          "last 2 versions",
          "safari >= 8",
          "ie >= 9",
          "ios >= 8"
        ]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    [
      "veui",
      {
        "modules": [
          {
            "package": "veui-theme-x",
            "fileName": "${module}.less"
          },
          {
            "package": "veui-theme-x",
            "fileName": "${module}.js",
            "transform": false
          }
        ]
      }
    ],
    "transform-decorators-legacy",
    "transform-vue-jsx",
    "transform-runtime"
  ],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["istanbul"]
    }
  }
}
